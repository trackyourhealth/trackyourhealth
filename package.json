    "docker:dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "env:sync": "sync-dotenv",
    "format:package": "format-package -w",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "nx": "nx",
    "nx:update": "nx migrate latest",
    "nx:update:migrate": "nx migrate --run-migrations",
    "prepare": "husky install",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "start": "nx serve",
    "test": "nx test"
  },
